<h3>Student Form</h3>

<%# Upon "Submit Post", action directs us to "/posts" %>
<%# This form will use the POST http verb. %>

<%= form_tag students_path do%>
  <label>First Name:</label><br>
  <input type="text" id="first_name" name="student[first_name]"> <br>
 
  <label>Last Name:</label><br>
  <input type="text" id="last_name" name="student[last_name]"> <br>
 
  <input type="submit" value="Submit Student">

  <% end %>